{
    "name": "phalcon/incubator-session",
    "description": "Phalcon Incubator Sessions Adapters",
    "keywords": [
        "framework",
        "phalcon",
        "incubator",
        "session"
    ],
    "homepage": "https://phalcon.io",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Phalcon Team",
            "email": "team@phalcon.io",
            "homepage": "https://phalcon.io/en/team"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/phalcon/incubator-session/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/phalcon/incubator/issues",
        "source": "https://github.com/phalcon/incubator-session",
        "forum": "https://forum.phalcon.io"
    },
    "require": {
        "php": ">=8.1",
        "ext-phalcon": "^5.0"
    },
    "require-dev": {
        "ext-mongodb": "*",
        "phalcon/ide-stubs": "^5.0",
        "vimeo/psalm": "^6.13",
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^10.0",
        "mongodb/mongodb": "^2.1",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Phalcon\\Incubator\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            "/.docker",
            "/.github",
            "/tests",
            ".codecov.yml",
            "docker-compose.yml",
            "phpcs.xml.dist",
            "psalm.xml"
        ]
    }
}
